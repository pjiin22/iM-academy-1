] = GG['업종명'].replace('의료·정밀기기', '의료정밀')\n","\n","GG['업종명'] = GG['업종명'].replace('금속', '철강금속')\n","\n","GG['업종명'] = GG['업종명'].replace('운송장비·부품', '운수장비')\n","\n","GG['업종명'] = GG['업종명'].replace('건설', '건설업')\n","\n","GG['업종명'] = GG['업종명'].replace('기타제조', '제조업')\n","\n","GG['업종명'] = GG['업종명'].replace('기타제조업', '제조업')\n","\n","GG['업종명'] = GG['업종명'].replace('금융', '금융업')\n","\n","GG['업종명'] = GG['업종명'].replace('섬유·의류', '섬유의복')\n","\n","GG['업종명'] = GG['업종명'].replace('비금속', '비금속광물')\n","\n","GG['업종명'] = GG['업종명'].replace('종이·목재', '종이목재')\n","\n","GG['업종명'] = GG['업종명'].replace('전기·가스·수도\t', '전기가스업')"],"metadata":{"id":"nhJ2io3a5yBK"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# 지수명과 업종명이 동일한 경우, A의 PER 값을 GG의 PER에 채워 넣기\n","def fill_per_based_on_industry(row, A):\n","    if pd.isna(row['PER']):\n","        matching_row = A[A['지수명'] == row['업종명']]\n","        if not matching_row.empty:\n","            return matching_row['PER'].values[0]\n","    return row['PER']\n","\n","GG['PER'] = GG.apply(fill_per_based_on_industry, axis=1, A=A)"],"metadata":{"id":"zid3D0Ffw2k_"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["GG"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":423},"id":"LwR6WtZQxjIv","executionInfo":{"status":"ok","timestamp":1724143038782,"user_tz":-540,"elapsed":513,"user":{"displayName":"박지인","userId":"12686226322425106492"}},"outputId":"1c847f93-a864-4335-a719-6283e325be12"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["       Code        ISU_CD    Name  Market     Dept   Close  ChangeCode  \\\n","0       660  KR7000660001  SK하이닉스   KOSPI      NaN  199700           1   \n","1     35720  KR7035720002     카카오   KOSPI      NaN   36550           2   \n","2     15760  KR7015760002    한국전력   KOSPI      NaN   19270           1   \n","3    402340  KR7402340004   SK스퀘어   KOSPI      NaN   88100           1   \n","4     34730  KR7034730002      SK   KOSPI      NaN  138500   